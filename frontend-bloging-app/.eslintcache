, setSlug] = useState(window.location.pathname.split('/'))\n    \n    const [posts, setPosts] = useState()\n    const [following, setFollowing] = useState()\n    const [follower, setFollower] = useState()\n    const [profile, setProfile] = useState({\n        \"name\": \"\",\n        \"profile_pic\": \"\",\n        \"email\": \"\",\n        \"about\": \"\"\n    })\n    \n    let showPosts = 'Loading...'\n    let showFollowing = 'Loading...'\n    let showFollower = 'Loading...'\n    let showProfile = 'Loading...'\n\n    useEffect(()=>{\n\n        var pathArray = window.location.pathname.split('/')\n        setSlug(pathArray)\n\n        // My Posts\n        axios({\n            method: 'get',\n            url: `http://127.0.0.1:8000/api/post/user/${slugLink[2]}`,\n            headers:{\n                'x-csrftoken': getCookie('csrftoken'),\n                'content-type': 'application/json'\n            }\n        }).then(response=>{\n            setPosts(response.data.data)\n        }).catch(e=>{\n            console.log(e)\n        })\n\n        // -- User Profile\n        axios({\n            method: 'get',\n            url: `http://127.0.0.1:8000/api/user/profile/${slugLink[2]}`,\n            headers:{\n                'x-csrftoken': getCookie('csrftoken'),\n                'content-type': 'application/json'\n            }\n        }).then(response=>{\n            setProfile(response.data.profile)\n        }).catch(e=>{\n            console.log(e)\n        })\n\n        // Following people\n        axios({\n            method: 'get',\n            url: `http://127.0.0.1:8000/api/user/following/${slugLink[2]}`,\n            headers:{\n                'x-csrftoken': getCookie('csrftoken'),\n                'content-type': 'application/json'\n            }\n        }).then(response=>{\n            setFollowing(response.data.following)\n        }).catch(e=>{\n            console.log(e)\n        })\n\n        // -- Followers\n        axios({\n            method: 'get',\n            url: `http://127.0.0.1:8000/api/user/follower/${slugLink[2]}`,\n            headers:{\n                'x-csrftoken': getCookie('csrftoken'),\n                'content-type': 'application/json'\n            }\n        }).then(response=>{\n            setFollower(response.data.follower)\n        }).catch(e=>{\n            console.log(e)\n        })\n\n    },[])\n\n    if (posts){\n        showPosts = posts.map((element)=>{\n            return <Posts post={element}/>\n        })\n    }\n\n    if(following){\n        showFollowing = following.map(element=>{\n            return <UserCard data={element}/>\n        })\n    }\n\n    if(follower){\n        showFollower = follower.map(element=>{\n            return <UserCard data={element}/>\n        })\n    }\n\n    if(profile){\n        showProfile = <UserHeader data={profile} user={slugLink[2]}/>\n    }\n\n    let pathFollowing = /\\/(follower-profile)\\/.*\\/(following)/\n    let pathFollower = /\\/(follower-profile)\\/.*\\/(followers)/\n\n    return(\n        <div className='container '>\n            <div className=\"row\">\n\n                <div className=\"col-4 col-lg-3 pt-5\">\n                </div>\n\n                <div className=\"col-8 col-lg-6 pt-5 mt-5 border rounded\">\n                <Router>\n                    {showProfile}\n                    <Switch>\n                        <Route exact path='/follower-profile/:slug'>\n                            {showPosts}\n                        </ Route>\n                        <Route exact path={pathFollowing}>\n                            {showFollowing}\n                        </ Route>\n                        <Route exact path={pathFollower} >\n                            {showFollower}\n                        </ Route>\n                    </Switch>\n\n                </Router>\n\n                </div>\n                <div className=\"col-0 col-lg-3\"></div>\n            </div>\n        </div>\n    )\n}\n\nexport default FollowerProfile\n\n\n","/mnt/d/files/Projects/6.BlogRest/frontend-bloging-app/src/Views/Logout.js",[],"/mnt/d/files/Projects/6.BlogRest/frontend-bloging-app/src/Views/Login.js",[],"/mnt/d/files/Projects/6.BlogRest/frontend-bloging-app/src/Components/posts.js",[],"/mnt/d/files/Projects/6.BlogRest/frontend-bloging-app/src/Components/ProfileCard.js",[],"/mnt/d/files/Projects/6.BlogRest/frontend-bloging-app/src/Components/PostSection.js",[],"/mnt/d/files/Projects/6.BlogRest/frontend-bloging-app/src/Views/getCSRF.js",[],["77","78"],"/mnt/d/files/Projects/6.BlogRest/frontend-bloging-app/src/Components/UserHeader.js",[],["79","80"],"/mnt/d/files/Projects/6.BlogRest/frontend-bloging-app/src/Components/getCookie.js",[],"/mnt/d/files/Projects/6.BlogRest/frontend-bloging-app/src/Components/userCard.js",[],"/mnt/d/files/Projects/6.BlogRest/frontend-bloging-app/src/Views/Register.js",[],{"ruleId":"81","replacedBy":"82"},{"ruleId":"83","replacedBy":"84"},{"ruleId":"85","severity":1,"message":"86","line":97,"column":7,"nodeType":"87","endLine":97,"endColumn":9,"suggestions":"88"},{"ruleId":"81","replacedBy":"89"},{"ruleId":"83","replacedBy":"90"},{"ruleId":"81","replacedBy":"91"},{"ruleId":"83","replacedBy":"92"},"no-native-reassign",["93"],"no-negated-in-lhs",["94"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'slugLink'. Either include it or remove the dependency array.","ArrayExpression",["95"],["93"],["94"],["93"],["94"],"no-global-assign","no-unsafe-negation",{"desc":"96","fix":"97"},"Update the dependencies array to be: [slugLink]",{"range":"98","text":"99"},[2681,2683],"[slugLink]"]